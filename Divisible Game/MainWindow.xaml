<Window x:Class="Divisible_Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Divisible_Game"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush/>
        </Grid.Background>
        <Label Content="Divisible Game" HorizontalAlignment="Left" TextBoxBase.TextChanged="Timer_TextChanged" TextBlock.FontSize="50" Margin="200,144,0,0" VerticalAlignment="Top" Height="104" Width="362"/>
        <Button x:Name="Play" Content="Play" HorizontalAlignment="Left" Margin="296,253,0,0" VerticalAlignment="Top" Width="160" Height="70" BorderBrush="Aquamarine" Background="Aquamarine" FontWeight="Bold" FontSize="36" Cursor="Pen"/>

    </Grid>
</Window>
